package com.example.bike;

public class Bike {
    String bikeName;
    int engineCC;
    double price;
    
    public static void main(String[] args) {
        Bike b = new Bike();
        b.bikeName = "Pulsar NS200";
        b.engineCC = 200;
        b.price = 145000.0;
        System.out.println("Bike: " + b.bikeName);
        System.out.println("Engine CC: " + b.engineCC);
        System.out.println("Price: â‚¹" + b.price);
    }
}
