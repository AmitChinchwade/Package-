package com.example.car;

public class Car {
    String brand;
    String model;
    double price;
    
    public static void main(String[] args) {
        Car c = new Car();
        c.brand = "Toyota";
        c.model = "Camry";
        c.price = 2500000.0;
        System.out.println("Brand: " + c.brand);
        System.out.println("Model: " + c.model);
        System.out.println("Price: â‚¹" + c.price);
    }
}
